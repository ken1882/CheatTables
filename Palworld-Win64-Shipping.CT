function CheatTableInterface.toggleCompactMode(sender, force)
    local newState
    if force ~= nil then
        newState = force
    else
        newState = not CheatTableInterface.CompactModeEnabled
    end

    CheatTableInterface.CompactModeEnabled = newState

    local mainForm = getMainForm()
    local controlsToToggle = {mainForm.Splitter1, mainForm.Panel4, mainForm.Panel5}

    -- Toggle visibility for controls
    for _, control in ipairs(controlsToToggle) do
        control.Visible = not newState
    end

    -- Transparency toggle
    mainForm.Visible = true
    mainForm.setLayeredAttributes(0x0000FE, newState and 240 or 255, 3)

    -- Address list header toggle
    AddressList.Header.Visible = not newState
end

