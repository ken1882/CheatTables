function CheatTableInterface.getProcess()
    return process
end

function CheatTableInterface.getProcessTrimmed()
    local procTrimmed = process:gsub('%.exe$', '')
    return procTrimmed
end

function CheatTableInterface.getGameModule()
    local modules = enumModules()
    if modules == nil or modules[1] == nil then return end
    return modules[1]
end

function CheatTableInterface.getGameModuleIs64Bit()
    local gm = CheatTableInterface.getGameModule()
    if gm then
        return gm.Is64Bit
    end
end

function CheatTableInterface.getGameModuleName()
    local gm = CheatTableInterface.getGameModule()
    if gm then
        return gm.Name
    end
end

function CheatTableInterface.getGameModulePathToFile()
    local gm = CheatTableInterface.getGameModule()
    if gm then
        return gm.PathToFile
    end
end

function CheatTableInterface.getGameModuleAddress()
    local gm = CheatTableInterface.getGameModule()
    if gm then
        return gm.Address
    end
end

function CheatTableInterface.getGameModuleInfo()
    return CheatTableInterface.getGameModuleName(),
           CheatTableInterface.getGameModuleIs64Bit(),
           CheatTableInterface.getGameModulePathToFile(),
           CheatTableInterface.getGameModuleAddress(),
           CheatTableInterface.getGameVersion()
end

function CheatTableInterface.getGameModuleInfoStrs()
    return CheatTableInterface.getGameModuleName(),
           CheatTableInterface.getGameModuleBitSizeStr(),
           CheatTableInterface.getGameModulePathToFile(),
           CheatTableInterface.getGameModuleAddress(),
           CheatTableInterface.getGameVersionStr()
end

function CheatTableInterface.getGameVersion()
    return getFileVersion(CheatTableInterface.getGameModulePathToFile())
end

function CheatTableInterface.getGameVersionStr()
    return CheatTableInterface.getFileVersionStr(CheatTableInterface.getGameModulePathToFile())
end

function CheatTableInterface.getFileVersionStr(path)
	if path == nil then return end
	local i, vt = getFileVersion(path)
	if i and vt then
		return string.format('%s.%s.%s.%s', vt.major, vt.minor, vt.release, vt.build)
	end
	return nil
end

function CheatTableInterface.getPointerSize()
    if function() return enumModules()[1].is64Bit end then
        return 0x8
    else
        return 0x4
    end
end

function CheatTableInterface.getRegistrySizeStr()
	if CheatTableInterface.getPointerSize() == 0x4 then
		return '(x32)'
	else
		return '(x64)'
	end
