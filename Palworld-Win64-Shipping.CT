function CheatTableInterface.setupTable(flags)
	if not inMainThread() then
		synchronize(function(thread)
			CheatTableInterface.setupTable(flags)
		end)
		return
	end

    -- Disable Drag and Drop
    if flags.disableDragAndDrop then
        CheatTableInterface.disableDragDrop()
    end

    -- Disable Header Sorting
    if flags.disableHeaderSorting then
        CheatTableInterface.disableHeaderSorting()
    end

    -- Add compact menu button
    if flags.addCompactMenu then
		CheatTableInterface.addCompactModeMenu()
	end

    -- Enable Compact Mode
    if flags.enableCompactMode then
        CheatTableInterface.toggleCompactMode(nil, true)
    end

    -- Load Default Color
    if flags.loadDefaultColor then
        CheatTableInterface.loadDefaultColor()
    end

    if flags.loadFoundListColoring then
        CheatTableInterface.loadFoundListColoring()
    end

    -- Set Title
    if flags.setTitle then
        CheatTableInterface.setTitle(tableInfo)
    end

    -- Hide "Table Extras"
    if flags.hideTableExtras then
        CheatTableInterface.hideTableExtras()
    end

    -- Hide "Advanced Options"
    if flags.hideAdvancedOptions then
        CheatTableInterface.hideAdvancedOptions()
    end

    if flags.hideHeaderSections then
        CheatTableInterface.hideHeaderSections()
    end

    -- Show Slogan
    if flags.showSlogan then
        CheatTableInterface.initializeSlogan()
    end

    -- Show Signature
    if flags.showSignature then
        CheatTableInterface.initializeSignature()
    end

    if flags.createHokteys then
        CheatTableInterface.setupHotkeys()
    end

    -- Auto Attach
    if flags.autoAttach then
      CheatTableInterface.autoAttach(tableInfo.processName, false)
    end
end

