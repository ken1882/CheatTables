    -- Set text color based on static/non-static
    local textColor = isStatic and defaultColor.Foundlist3_Static_Text_Color or defaultColor.Foundlist3_NonStatic_Text_Color

    Sender.Canvas.Font.Color = textColor
    return true  -- Indicate custom drawing is handled
end

-- Handle custom drawing of subitems (value column)
function CheatTableInterface.customDrawSubItem(Sender, Item, SubItemIndex, State)
    -- Item.SubItems are [0] Value, [1] Last Scan, [2] First
    -- SubItemIndex are [0] Address, [1] Value, [2] Last Scan, [3] First
    local textColor = nil
    local subItemCount = Item.SubItems.Count

    if SubItemIndex == 1 then
        -- Compare current value with previous value
        local value = Item.SubItems[0] or ""
        local previous = (subItemCount &gt; 1) and Item.SubItems[1] or ""
        textColor = (value ~= previous) and defaultColor.Foundlist3_Changed_Text_Color or defaultColor.Foundlist3_Default_Text_Color

    elseif SubItemIndex == 2 then
        -- Compare previous value with first value
        local previous = (subItemCount &gt; 1) and Item.SubItems[1] or ""
        local first = (subItemCount &gt; 2) and Item.SubItems[2] or ""
        textColor = (previous ~= first) and defaultColor.Foundlist3_Changed_Text_Color or defaultColor.Foundlist3_Default_Text_Color

    elseif SubItemIndex == 3 then
        -- Compare current value with first value
        local value = Item.SubItems[0] or ""
        local first = (subItemCount &gt; 2) and Item.SubItems[2] or ""
        textColor = (value ~= first) and defaultColor.Foundlist3_Changed_Text_Color or defaultColor.Foundlist3_Default_Text_Color
    end

    Sender.Canvas.Font.Color = textColor

    return true  -- Indicate custom drawing is handled
end

