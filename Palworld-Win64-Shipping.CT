function CheatTableInterface.extractAndWriteColorComponents()

  local itemIndex = readByte("ItemIndex")

  local colorValue
  if itemIndex == 0 then
    colorValue = getAddressList().Control[0] and getAddressList().Control[0].BackgroundColor or defaultColor.Control_BackgroundColor
  elseif itemIndex == 1 then
    colorValue = getAddressList().CheckboxColor or defaultColor.CheckboxColor
  elseif itemIndex == 2 then
    colorValue = getMainForm().Foundlist3 and getMainForm().Foundlist3.Color or defaultColor.Foundlist3_Color
  elseif itemIndex == 3 then
    colorValue = getMainForm().color or defaultColor.mainForm_Color
  else
    -- Handle invalid item index
    return nil
  end

  colorValue = string.format("%08X", colorValue)

  local red = tonumber(colorValue:sub(7, 8), 16)
  local green = tonumber(colorValue:sub(5, 6), 16)
  local blue = tonumber(colorValue:sub(3, 4), 16)

  writeInteger("RedValue", red)
  writeInteger("GreenValue", green)
  writeInteger("BlueValue", blue)

  return red, green, blue
end

function CheatTableInterface.setColorState(colorState)
  local addressList = getAddressList()
  local mainForm = getMainForm()

  if addressList.Control[0] then
    addressList.Control[0].BackgroundColor = colorState.Control_BackgroundColor
  end

  addressList.CheckboxColor = colorState.CheckboxColor

  if mainForm.Foundlist3 then
    mainForm.Foundlist3.Color = colorState.Foundlist3_Color
  end

  mainForm.color = colorState.mainForm_Color
end

function CheatTableInterface.writeColorStateToFile(fileName)
  local filePath = string.format("~%s_%s.txt", CheatTableInterface.getProcessTrimmed(), fileName)
  local file = io.open(filePath, "w")

  local colorState = CheatTableInterface.getColorState()

  if file then
    file:write(string.format("Control_BackgroundColor=%06X\n", colorState.Control_BackgroundColor))
    file:write(string.format("CheckboxColor=%06X\n", colorState.CheckboxColor))
    file:write(string.format("Foundlist3_Color=%06X\n", colorState.Foundlist3_Color))
    file:write(string.format("mainForm_Color=%06X\n", colorState.mainForm_Color))

    file:close()
    return true
  else
    return false
  end
end

function CheatTableInterface.readColorStateFromFile(fileName)
  local filePath = string.format("~%s_%s.txt", CheatTableInterface.getProcessTrimmed(), fileName)
  local file = io.open(filePath, "r")

  if file then
    local colorState = CheatTableInterface.getColorState()

    for line in file:lines() do
      local key, value = line:match("([^=]+)=(.+)")

      if key and value then
        colorState[key] = tonumber(value, 16)
      end
    end

    CheatTableInterface.setColorState(colorState)

    file:close()
	getMainForm().repaint()
    return colorState
  else
    return nil
  end
end

function CheatTableInterface.applyColorState()
    local redValue = readByte("RedValue")
    local greenValue = readByte("GreenValue")
    local blueValue = readByte("BlueValue")
    local itemIndexValue = readByte("ItemIndex")

    local hexValue = string.format("%02X%02X%02X", blueValue, greenValue, redValue)
    if itemIndexValue == 0 then
        getAddressList().Control[0].BackgroundColor = '$00' .. hexValue
    elseif itemIndexValue == 1 then
        getAddressList().CheckboxColor = '$00' .. hexValue
    elseif itemIndexValue == 2 then
        getMainForm().Foundlist3.Color = '$00' .. hexValue
    elseif itemIndexValue == 3 then
        getMainForm().color = '$00' .. hexValue
    end
end

