function hex_to_question_marks(hex_string)
    -- Remove spaces and convert to uppercase
    hex_string = hex_string:gsub("%s+", ""):upper()

    local result = {}

    -- Process each pair of hex characters
    for i = 1, #hex_string, 2 do
        table.insert(result, "??")
    end

    return table.concat(result, " ")
end

function convertClipboardHexToQuestionMarks()
    if not inMainThread() then
        synchronize(function(thread)
            convertClipboardHexToQuestionMarks()
        end)
        return
    end

    local clipboardText = readFromClipboard()
    if not clipboardText or clipboardText == "" then
        debugNotif("Clipboard is empty", "No Content")
        return
    end

    -- Check if it looks like hex
    if not clipboardText:match("^[%s0-9A-Fa-f]+$") then
        debugNotif("Clipboard content doesn't appear to be hex format:\n" .. clipboardText, "Invalid Format")
        return
    end

    local converted = hex_to_question_marks(clipboardText)
    writeToClipboard(converted)
    -- debugNotif("Converted hex to question marks:\n" .. clipboardText .. "\n\n" .. converted, "Hex Converted")
end

