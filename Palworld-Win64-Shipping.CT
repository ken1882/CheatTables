-- Function to check if the required symbols exist and are valid
function checkSymbolsExist()
    local hasBasePtr = getAddressSafe(CONFIG.basePtrName) ~= nil

    if not hasBasePtr then
        notification("Required symbol '" .. CONFIG.basePtrName .. "' not found.\nEnsure the ASM portion is activated.", "Error")
        return false
    end

    -- Check if base pointer is populated
    local basePtr = readQword(CONFIG.basePtrName)
    if basePtr == 0 then
        notification("Position base pointer is invalid.\nMove in-game to populate values.", "Error")
        return false
    end

    return true
end

-- Function to save the current position
function savePosition()
    -- Check if symbols exist and are valid
    if not checkSymbolsExist() then
        return
    end

    local coords = getCurrentCoordinates()
    if not coords then
        notification("Position data is empty or invalid. Move around first to populate coordinates.", "Error")
        return
    end

    -- Check if coordinates are all zero
    local allZero = true
    for _, component in ipairs(SETTINGS.componentNames) do
        if coords[component] ~= 0 then
            allZero = false
            break
        end
    end

    if allZero then
        notification("Position data is empty or invalid. Move around first to populate coordinates.", "Error")
        return
    end

    -- Ask user for position name
    local name = getTextInput("Enter a name for this position:", "Save Position")

    if not name or name == "" then
        notification("Save cancelled.", "Info")
        return
    end

    -- Load existing positions to check if this one exists
    local positions = loadPositions()

    -- Add 20.0 to Y coordinate to avoid spawning in ground (if Y exists)
    if coords["z"] then
        coords["z"] = coords["z"] + 50.0
    elseif coords["y"] then
        coords["y"] = coords["y"] + 50.0
    end

    -- Format coordinates for display
    local coordDisplay = ""
    for i, component in ipairs(SETTINGS.componentNames) do
        coordDisplay = coordDisplay .. component .. ": " .. coords[component]
        if i &lt; #SETTINGS.componentNames then
            coordDisplay = coordDisplay .. "\n"
        end
    end

    -- Check if position already exists
    if positions[name] then
        notification(
            "Position '" .. name .. "' already exists. Add as new point or overwrite?",
            "Position Exists",
            {"Add Point", "Overwrite", "Cancel"},
            function(buttonClicked)
                if buttonClicked == "Add Point" then
                    -- Add new point to existing position
                    if savePositionToFile(name, coords, true) then
                        notification("New point added to '" .. name .. "' successfully!\n" .. coordDisplay, "Success")
                    end
                elseif buttonClicked == "Overwrite" then
                    -- Overwrite position
                    if savePositionToFile(name, coords, false) then
                        notification("Position '" .. name .. "' overwritten successfully!\n" .. coordDisplay, "Success")
                    end
                end
            end
        )
    else
        -- Create new position with single point
        if savePositionToFile(name, coords, false) then
            notification("Position '" .. name .. "' saved successfully!\n" .. coordDisplay, "Success")
        end
    end
end

