        ceRegistrySizeStr,
        getCEVersion())

    getMainForm().Caption = titleStr
    return titleStr
end

function CheatTableInterface.autoAttach(processName)
	processName = processName or tableInfo.processName

    -- Check if already attached to the correct process; necessary, because recursive loop with setupTable
    if getProcessIDFromProcessName(processName) == getOpenedProcessID() then
        if setupFlags.releaseSymbols == true then
            local releaseTimer = createTimer(MainForm)
            releaseTimer.Interval = 10000
            releaseTimer.OnTimer = function(timer)
                releaseDebugFiles()
                timer.destroy()
            end
        end
        return
    end

	local function autoAttachTimer_tick(timer)
		if CheatTableInterface.AutoAttachTimerTickMax &gt; 0
			and CheatTableInterface.AutoAttachTimerTicks &gt;= CheatTableInterface.AutoAttachTimerTickMax then
			timer.destroy()
		end
		local processID = getProcessIDFromProcessName(processName)
		if processID ~= nil then
			timer.destroy()
			openProcess(processID)
			CheatTableInterface.setupTable(setupFlags)
		end
		CheatTableInterface.AutoAttachTimerTicks = CheatTableInterface.AutoAttachTimerTicks + 1
	end

	local autoAttachTimer = createTimer(MainForm)
	autoAttachTimer.Interval = CheatTableInterface.AutoAttachTimerInterval
	autoAttachTimer.OnTimer = autoAttachTimer_tick
end

function CheatTableInterface.autoDisable(id, customInterval)
    if not inMainThread() then
        synchronize(function(thread)
            CheatTableInterface.autoDisable(id, customInterval)
        end)
        return
    end
    checkSynchronize()

    local function autoDisableTimer_tick(timer)
        timer.destroy()
        local mr = AddressList.getMemoryRecordByID(id)
        if mr ~= nil and mr.Active then
            while mr.Async and mr.AsyncProcessing do
                sleep(0)
            end
            mr.Active = false
            while mr.Async and mr.AsyncProcessing do
                checkSynchronize()
                MainForm.repaint()
                sleep(0)
            end
        end
    end

    local autoDisableTimer = createTimer(MainForm)
    autoDisableTimer.Interval = customInterval or CheatTableInterface.AutoDisableTimerInterval
    autoDisableTimer.OnTimer = autoDisableTimer_tick
end

---@description Toggles compact mode for the Cheat Engine interface
