---@description Sets up pattern scanning with automatic saving/loading
---@param pattern string The byte pattern to search for (e.g., "48 8B ?? ?? ?? ?? ??")
---@param name string Unique identifier for this pattern
---@param targetIndex number Which result to use (1-based index)
---@param moduleName string|nil Module to scan within (nil for full memory)
---@param many boolean Whether to find multiple results
---@return boolean success True if pattern was found and registered
function setupPattern(pattern, name, targetIndex, moduleName, many)
    many = (many == nil) and true or many
    targetIndex = targetIndex or 1

    -- Try to read saved locations first
    local saved, savedBytes, isRelative = readSavedLocation(name, moduleName)
    if saved then
        local normalizedSavedBytes = string.gsub(savedBytes, "%s+", "")
        local normalizedPattern = string.gsub(pattern, "%s+", "")

        if checkBytesWithWildcards(normalizedSavedBytes, normalizedPattern) then
            if saved[targetIndex] then
                -- Convert relative to absolute if needed
                local address = saved[targetIndex]
                if isRelative and moduleName then
                    local module = getAddressSafe(moduleName)
                    if module then
                        address = module + address
                    else
                        debugPrint("Module not found for relative address: " .. moduleName)
                        debugNotif("Module not found for relative address: " .. moduleName, "Error")
                        return false
                    end
                end
                registerSymbol(name, address)
                return true
            else
                debugPrint("Saved location exists but target index " .. targetIndex .. " is invalid")
                local validIndices = #saved
                debugNotif(
                    "Target index " .. targetIndex .. " is invalid. Valid indices are 1-" .. validIndices .. ".",
                    "Invalid Index",
                    {"Cancel", "Use First Valid Index"},
                    function(buttonClicked)
                        if buttonClicked == "Use First Valid Index" then
                            -- Rerun the function with index 1
                            debugPrint("Retrying with index 1")
                            return setupPattern(pattern, name, 1, moduleName, many)
                        end
                    end
                )
                return false
            end
        else
            debugPrint("Saved pattern doesn't match expected pattern for: " .. name)
            debugPrint("Expected: " .. pattern)
            debugPrint("Found: " .. savedBytes)
            debugNotif("Saved pattern doesn't match expected pattern for: " .. name, "Pattern Mismatch")
        end
    else
        debugPrint("No Saved Found.")
        debugNotif("No saved location found for " .. name, "Scanning Required")
    end

    -- If not found or invalid, scan for it
    local resultList
    local scanFlags = "*W+X"
    if not many then
        scanFlags = scanFlags .. "+f" -- Add first-occurrence flag only when needed
    end
    local moduleAddress = nil

    if moduleName then
        moduleAddress = getAddressSafe(moduleName)
        if moduleAddress then
            local startAddress = moduleAddress
            local endAddress = moduleAddress + getModuleSize(moduleName)

            resultList = AOBScan(pattern, scanFlags, startAddress, endAddress)

            -- Close scanning form
            closeScanningForm()
        else
            debugPrint("Module not found: " .. moduleName)
            debugNotif("Module not found: " .. moduleName, "Error")
            return false
        end
    else

        resultList = AOBScan(pattern, scanFlags)
    end

    -- Close scanning form
    closeScanningForm()

    debugPrint("Result type: " .. type(resultList))

    if not resultList then
        debugPrint("Pattern not found.")
        debugNotif("Pattern not found.", "Scan Failed")
        return false
    else
        local results = resultList.getCount()
        debugPrint("Found " .. results .. " locations")
        local addresses = {}
        local useRelative = moduleName ~= nil and moduleAddress ~= nil

        -- Store all addresses
        for i = 0, results - 1 do
            local addrStr = resultList[i]
            if type(addrStr) == "string" then
                -- Convert hex string to number
                local addr = tonumber(addrStr, 16)
                if addr then
                    if useRelative then
                        -- Store relative to module base
                        addresses[i + 1] = addr - moduleAddress
                    else
                        addresses[i + 1] = addr
                    end
                else
                    debugPrint("Warning: Could not convert result at index " .. i .. " to number: " .. addrStr)
                    addresses[i + 1] = 0
                end
            else
                -- Handle directly if it's already a number
                local addr = addrStr
                if useRelative then
                    addresses[i + 1] = addr - moduleAddress
                else
                    addresses[i + 1] = addr
                end
            end
        end

        -- Register the target index specifically
        if targetIndex &lt;= results then
            -- Always register absolute address
            local registerAddr = addresses[targetIndex]
            if useRelative then
                registerAddr = registerAddr + moduleAddress
            end
            registerSymbol(name, registerAddr)
        else
            debugPrint("Results found, but Index " .. targetIndex .. " is invalid")
            local validIndices = results
            debugNotif(
                "Target index " .. targetIndex .. " is invalid. Valid indices are 1-" .. validIndices .. ".",
                "Invalid Index",
                {"Cancel", "Use First Valid Index"},
                function(buttonClicked)
                    if buttonClicked == "Use First Valid Index" then
                        -- Rerun the function with index 1
                        debugPrint("Retrying with index 1")
                        return setupPattern(pattern, name, 1, moduleName, many)
                    end
                end
            )
            return false
        end

        -- Save all locations
        saveLocation(name, addresses, pattern, useRelative, moduleName)

        local resultMessage = "Found and saved " .. results .. " locations for: " .. name
        debugPrint(resultMessage)

        local targetAddr = addresses[targetIndex]
        local addressInfo = ""
        if useRelative then
            addressInfo = "Target relative address at index " .. targetIndex .. ": " .. string.format("%X", targetAddr) ..
                         "\nTarget absolute address at index " .. targetIndex .. ": " .. string.format("%X", targetAddr + moduleAddress)
            debugPrint("Target relative address at index " .. targetIndex .. ": " .. string.format("%X", targetAddr))
            debugPrint("Target absolute address at index " .. targetIndex .. ": " .. string.format("%X", targetAddr + moduleAddress))
        else
            addressInfo = "Target address at index " .. targetIndex .. ": " .. string.format("%X", targetAddr)
            debugPrint("Target address at index " .. targetIndex .. ": " .. string.format("%X", targetAddr))
        end

        debugNotif(resultMessage .. "\n\n" .. addressInfo, "Scan Complete", {"OK", "View All Addresses"}, function(button)
            if button == "View All Addresses" then
                local allAddresses = "All found addresses:\n\n"
                for i, addr in ipairs(addresses) do
                    if useRelative then
                        allAddresses = allAddresses .. i .. ": " .. string.format("%X", addr) ..
                                      " (absolute: " .. string.format("%X", addr + moduleAddress) .. ")\n"
                    else
                        allAddresses = allAddresses .. i .. ": " .. string.format("%X", addr) .. "\n"
                    end
                end
                debugNotif(allAddresses, "All Addresses", {"Close", "Copy to Clipboard"}, function(btn)
                    if btn == "Copy to Clipboard" then
                        -- Copy to clipboard logic here
                        if getForegroundWindow()~=getCheatEngineForm() then
                            createStringlist().saveToFile(getTempFolder()..'\\addresses.txt')
                            shellExecute(getTempFolder()..'\\addresses.txt')
                        end
                        getMemoryViewForm().DisassemblerView.SelectedAddress = addresses[1]
                    end
                end)
            end
        end)

        return true
    end
end

-- Modified function to check bytes with wildcard support
function checkBytesWithWildcards(bytes, pattern)
    -- Remove spaces from both
    local cleanBytes = string.gsub(bytes, "%s+", "")
    local cleanPattern = string.gsub(pattern, "%s+", "")

    -- Make sure lengths match (after considering wildcards)
    local patternLen = #cleanPattern
    local bytesLen = #cleanBytes

    if patternLen ~= bytesLen then
        return false
    end

    -- Check byte by byte, skipping wildcards
    for i = 1, patternLen, 2 do
        local patternByte = cleanPattern:sub(i, i+1)
        local actualByte = cleanBytes:sub(i, i+1)

        -- Skip comparison for ** wildcard; as in, don't compare, when it's a wildcard
        if patternByte ~= "**" and patternByte ~= "??" then
            if patternByte ~= actualByte then
                return false
            end
        end
    end

    return true
end

-- Function to read saved AOB locations
function readSavedLocation(name, currentModuleName)
    local file = findTableFile(name)
    if not file then
       debugPrint("No File Found for name: " .. tostring(name))
       return nil
    end

    local lines = readTableFileContent(file)

    -- Check if we have enough data
    if #lines &lt; 2 then
       debugPrint("Missing lines")
       return nil
    end

    local addresses = {}
    local isRelative = false
    local savedModuleName = nil

    -- Check for module info in third line
    if #lines &gt;= 3 then
        local moduleInfo = lines[3]
        if string.find(moduleInfo, "rel:") == 1 then
            isRelative = true
            savedModuleName = string.sub(moduleInfo, 5)

            -- If module names don't match, we can't use relative addresses properly
            if currentModuleName and savedModuleName ~= currentModuleName then
                debugPrint("Warning: Saved module name doesn't match current module")
                debugPrint("Saved: " .. savedModuleName .. ", Current: " .. currentModuleName)
                return nil
            end
        end
    end

    -- First line contains all addresses separated by commas
    if string.find(lines[1], ",") then
        -- Multiple addresses separated by commas
        for addrStr in string.gmatch(lines[1], "[^,]+") do
            local addr = tonumber(addrStr, 16)
            if addr then
                table.insert(addresses, addr)
            end
        end
    else
        -- Single address
        local addr = tonumber(lines[1], 16)
        if addr then
            table.insert(addresses, addr)
        end
    end

    local savedBytes = lines[2]

    -- Verify bytes at each address match what we saved, with wildcard support
    for _, addr in ipairs(addresses) do
        -- Remove spaces from saved bytes pattern
        local cleanSavedBytes = string.gsub(savedBytes, "%s+", "")
        -- Calculate bytes to read based on cleaned pattern (count non-wildcard bytes)
        local bytesToRead = #cleanSavedBytes / 2

        local checkAddr = addr
        if isRelative and currentModuleName then
            local module = getAddressSafe(currentModuleName)
            if module then
                checkAddr = module + addr
            else
                debugPrint("Module not found: " .. currentModuleName)
                return nil
            end
        end

        local bytes = readBytes(checkAddr, bytesToRead, true)
        local currentBytes = bytesToString(bytes)

        if not currentBytes or not cleanSavedBytes then
           return nil
        end

        -- Use the wildcard-aware comparison
        if not checkBytesWithWildcards(currentBytes, cleanSavedBytes) then
            debugPrint("Bytes don't match!")
            debugNotif("Pattern does not Match.", "Matching Failed")
            return nil
        end
    end

    return addresses, savedBytes, isRelative
end

-- Function to save AOB locations
function saveLocation(name, addresses, pattern, isRelative, moduleName)
    local existingFile = findTableFile(name)
    if existingFile then
        debugPrint("Deleting existing file: " .. tostring(name))
        existingFile.Delete()
    end

    local file = createTableFile(name)
    if not file then
        debugPrint("Failed to create file: " .. tostring(name))
        return false
    end

    -- Create comma-separated string of addresses
    local addressesStr = ""
    for i, addr in ipairs(addresses) do
        addressesStr = addressesStr .. string.format("%X", addr)
        if i &lt; #addresses then
            addressesStr = addressesStr .. ","
        end
    end

    local content = addressesStr .. "\n" .. pattern

    -- Add module info if relative
    if isRelative and moduleName then
        content = content .. "\nrel:" .. moduleName
    end

    writeTableFileContent(file, content)
    return true
end

-- Helper function to convert bytes to string
function bytesToString(bytes)
    if not bytes then return nil end
    local str = ""
    for i=1, #bytes do
        str = str .. string.format("%02X", bytes[i])
    end
    return str
end

-- Helper function to read table file content
function readTableFileContent(file)
    local stream = createStringStream()
    stream.copyFrom(file.Stream, file.Stream.Size)

    local lines = {}
    for line in string.gmatch(stream.DataString, "[^\r\n]+") do
        table.insert(lines, line)
    end

    stream.destroy()
    return lines
end

-- Helper function to write table file content
function writeTableFileContent(file, content)
    local bytes = stringToByteTable(content)
    file.Stream.Position = 0
    file.Stream.write(bytes, #bytes)
end




