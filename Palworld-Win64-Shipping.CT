-- Global debug state
local DEBUG_MODE = false
local NOTIF_MODE = true
local scanningForm = nil  -- Track active scanning notification

-- Function to estimate string width without canvas
function estimateStringWidth(text, font)
    -- Multiplier based on typical fonts
    local multiplier = 0.55
    local fontSize = font and font.Size or 10

    -- Account for different character widths
    local effectiveLength = 0
    for i = 1, #text do
        local char = text:sub(i, i)
        if char:match("[iIl1.|]") then
            effectiveLength = effectiveLength + 0.5 -- narrow characters
        elseif char:match("[mwWM]") then
            effectiveLength = effectiveLength + 1.2 -- wide characters
        else
            effectiveLength = effectiveLength + 1 -- normal characters
        end
    end

    return effectiveLength * fontSize * multiplier
end

-- Count lines in text (for height calculation), includes word-wrap
function countTextLines(text)
    -- Count explicit line breaks (both \n and \r\n)
    local explicitLines = 1

    -- Count \n line breaks
    for _ in string.gmatch(text, '\n') do
        explicitLines = explicitLines + 1
    end

    -- Also look for standalone \r characters (without following \n)
    local rCount = select(2, string.gsub(text, '\r[^\n]', ''))
    explicitLines = explicitLines + rCount

    -- Word wrap estimation
    local baseWidth = 400  -- Base window width
    local words = {}
    for word in text:gmatch("%S+") do
        table.insert(words, word)
    end

    local wrappedLines = 0
    local currentLineWidth = 0
    local avgCharWidth = 6 -- Approximate average character width in pixels

    for _, word in ipairs(words) do
        local wordWidth = #word * avgCharWidth
        if currentLineWidth + wordWidth &gt; baseWidth then
            wrappedLines = wrappedLines + 1
            currentLineWidth = wordWidth
        else
            currentLineWidth = currentLineWidth + wordWidth + avgCharWidth -- word + space
        end
    end

    return explicitLines + wrappedLines
end

-- Notification function to display messages in a window
function notification(message, title, buttons, callback, autoClose)

    title = title or "Notification"
    buttons = buttons or {"OK"}

    -- Create form with fixed position to prevent overlap
    local form = createForm(false)
    form.Caption = title
    form.BorderStyle = 'bsSingle'

    -- Label creation with improved positioning
    local label = createLabel(form)
    label.Parent = form
    label.Left = 20
    label.Top = 20
    label.Caption = message
    label.WordWrap = true

    -- Better size calculations
    local estimatedWidth = estimateStringWidth(message, label.Font)
    local baseWidth = math.min(math.max(estimatedWidth, 300), 600)
    local estimatedLines = countTextLines(message)
    local textHeight = estimatedLines * (label.Font.Size + 6 ) *1.5 -- Added more vertical spacing

    -- Set dimensions
    label.Width = baseWidth
    label.Height = textHeight + 20 -- Extra padding

    -- Calculate button sizes with better accuracy
    local buttonWidths = {}
    local totalButtonWidth = 0
    local buttonSpacing = 10
    local buttonHeight = 30

    for i, buttonText in ipairs(buttons) do
        -- Calculate more accurate button widths based on text
        local textWidth = estimateStringWidth(buttonText, label.Font) + 40  -- More padding
        buttonWidths[i] = math.max(textWidth, 80)  -- Ensure minimum width
        totalButtonWidth = totalButtonWidth + buttonWidths[i]
    end

    totalButtonWidth = totalButtonWidth + buttonSpacing * (#buttons - 1)

    -- Set form dimensions with better vertical space
    form.Width = math.max(baseWidth + 40, totalButtonWidth + 40)
    form.Height = label.Top + textHeight + buttonHeight + 60 -- More vertical space

    -- Adjust label width
    label.Width = form.Width - 40

    -- Create buttons with improved positioning
    local startX = (form.Width - totalButtonWidth) / 2
    local currentX = startX
    local buttonY = label.Top + label.Height + 10 -- Position buttons below text

    for i, buttonText in ipairs(buttons) do
        local btn = createButton(form)
        btn.Parent = form
        btn.Caption = buttonText
        btn.Width = buttonWidths[i]
        btn.Height = buttonHeight
        btn.Left = currentX
        btn.Top = buttonY
        btn.Default = (i == 1)
        btn.Tag = buttonText

        btn.OnClick = function()
            if callback then
                callback(buttonText)
            end
            form.Close()
        end

        currentX = currentX + btn.Width + buttonSpacing
    end

    -- Center the form on screen
    form.Position = 'poScreenCenter'

    -- Track form for scanning notifications
    if title == "Scanning" then
        scanningForm = form
    end

    -- Show form with improved timing
    if autoClose then
        form.show()  -- Non-modal for auto-close windows
    else
        form.showModal()  -- Modal for user interaction required
    end

    return form
end

-- Function to close scanning form if active
function closeScanningForm()
    if scanningForm then
        scanningForm.Close()
        scanningForm = nil
    end
end

