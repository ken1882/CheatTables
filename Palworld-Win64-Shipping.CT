-- Function to load a position
function loadPosition()
    -- Check if symbols exist
    if not checkSymbolsExist() then
        return
    end

    -- Load positions
    local positions = loadPositions()

    -- Get selected position
    local selectedPos = selectPosition("Load Position", "load")

    if not selectedPos then return end

    -- Get position data
    local posList = positions[selectedPos]
    if not posList or #posList == 0 then
        notification("Failed to retrieve position data.", "Error")
        return
    end

    -- If multiple points, let user select which one
    local selectedPoint = 1
    if #posList &gt; 1 then
        -- Create form for point selection
        local form = createForm(false)
        form.Caption = "Select Point"
        form.BorderStyle = "bsSingle"
        form.Width = 300
        form.Height = 400
        form.Position = "poScreenCenter"

        -- Add listbox for points
        local listbox = createListBox(form)
        listbox.Left = 20
        listbox.Top = 20
        listbox.Width = 260
        listbox.Height = 300

        -- Populate listbox
        for i, pos in ipairs(posList) do
            local pointInfo = "Point " .. i
            for _, component in ipairs(SETTINGS.componentNames) do
                pointInfo = pointInfo .. " - " .. component .. ": " .. pos[component]
            end
            listbox.Items.add(pointInfo)
        end

        -- Add buttons
        local btnOK = createButton(form)
        btnOK.Caption = "OK"
        btnOK.Left = 110
        btnOK.Top = 330
        btnOK.Width = 80
        btnOK.Default = true
        btnOK.Enabled = false

        -- Add cancel button
        local btnCancel = createButton(form)
        btnCancel.Caption = "Cancel"
        btnCancel.Left = 200
        btnCancel.Top = 330
        btnCancel.Width = 80

        -- Enable OK button when selection changes
        listbox.OnClick = function()
            if listbox.ItemIndex &gt;= 0 then
                selectedPoint = listbox.ItemIndex + 1
                btnOK.Enabled = true
            else
                btnOK.Enabled = false
            end
        end

        -- Handle OK button click
        btnOK.OnClick = function()
            form.modalResult = mrOK
        end

        -- Handle Cancel button click
        btnCancel.OnClick = function()
            selectedPoint = nil
            form.modalResult = mrCancel
        end

        -- Show modal and wait for result
        form.showModal()

        -- Clean up the form
        form.destroy()

        if not selectedPoint then return end
    end

    -- Get selected position
    local pos = posList[selectedPoint]
    local basePtr = readQword(CONFIG.basePtrName)

    if basePtr == 0 then
        notification("Position pointer is invalid. Move around first.", "Error")
        return
    end

    -- Write values to memory
    for i, component in ipairs(SETTINGS.componentNames) do
        local offset = (i - 1) * SETTINGS.valueSize
        SETTINGS.writeValue(basePtr + offset, pos[component])
    end

    -- Format coordinates for display
    local coordDisplay = ""
    for i, component in ipairs(SETTINGS.componentNames) do
        coordDisplay = coordDisplay .. component .. ": " .. pos[component]
        if i &lt; #SETTINGS.componentNames then
            coordDisplay = coordDisplay .. "\n"
        end
    end

    notification("Position '" .. selectedPos .. "' (point " .. selectedPoint .. ") loaded!\n" .. coordDisplay, "Success")
end

-- Function to delete a position
function deletePosition()
    -- Load positions
    local positions = loadPositions()

    -- Get selected position
    local selectedPos = selectPosition("Delete Position", "delete")

    if not selectedPos then return end

    -- Check if position has multiple points
    if #positions[selectedPos] &gt; 1 then
        notification(
            "'" .. selectedPos .. "' has multiple points. Delete entire position or just one point?",
            "Delete Options",
            {"Delete All", "Delete Point", "Cancel"},
            function(buttonClicked)
                if buttonClicked == "Delete All" then
                    -- Delete entire position
                    if removePositionFromFile(selectedPos) then
                        notification("Position '" .. selectedPos .. "' deleted.", "Success")
                    end
                elseif buttonClicked == "Delete Point" then
                    -- Show point selection form
                    local form = createForm(false)
                    form.Caption = "Select Point to Delete"
                    form.BorderStyle = "bsSingle"
                    form.Width = 300
                    form.Height = 400
                    form.Position = "poScreenCenter"

                    -- Add listbox for points
                    local listbox = createListBox(form)
                    listbox.Left = 20
                    listbox.Top = 20
                    listbox.Width = 260
                    listbox.Height = 300

                    -- Populate listbox
                    for i, pos in ipairs(positions[selectedPos]) do
                        local pointInfo = "Point " .. i
                        for _, component in ipairs(SETTINGS.componentNames) do
                            pointInfo = pointInfo .. " - " .. component .. ": " .. pos[component]
                        end
                        listbox.Items.add(pointInfo)
                    end

                    -- Add buttons
                    local btnOK = createButton(form)
                    btnOK.Caption = "Delete"
                    btnOK.Left = 110
                    btnOK.Top = 330
                    btnOK.Width = 80
                    btnOK.Default = true
                    btnOK.Enabled = false

                    -- Add cancel button
                    local btnCancel = createButton(form)
                    btnCancel.Caption = "Cancel"
                    btnCancel.Left = 200
                    btnCancel.Top = 330
                    btnCancel.Width = 80

                    local selectedPoint = nil

                    -- Enable OK button when selection changes
                    listbox.OnClick = function()
                        if listbox.ItemIndex &gt;= 0 then
                            selectedPoint = listbox.ItemIndex + 1
                            btnOK.Enabled = true
                        else
                            btnOK.Enabled = false
                        end
                    end

                    -- Handle OK button click
                    btnOK.OnClick = function()
                        form.modalResult = mrOK
                    end

                    -- Handle Cancel button click
                    btnCancel.OnClick = function()
                        selectedPoint = nil
                        form.modalResult = mrCancel
                    end

                    -- Show modal and wait for result
                    form.showModal()

                    -- Clean up the form
                    form.destroy()

                    if selectedPoint then
                        if removePositionFromFile(selectedPos, selectedPoint) then
                            notification("Point deleted from '" .. selectedPos .. "'.", "Success")
                        end
                    end
                end
            end
        )
    else
        -- Single point position - confirm deletion
        notification(
            "Are you sure you want to delete '" .. selectedPos .. "'?",
            "Confirm Deletion",
            {"Yes", "No"},
            function(buttonClicked)
                if buttonClicked == "Yes" then
                    -- Delete position
                    if removePositionFromFile(selectedPos) then
                        notification("Position '" .. selectedPos .. "' deleted.", "Success")
                    end
                end
            end
        )
    end
end

-- Function to manually input position coordinates
function manualInputPosition()
    -- Create form for coordinate input
    local form = createForm(false)
    form.Caption = "Manual Position Input"
    form.BorderStyle = "bsSingle"
    form.Width = 320
    form.Height = 250 + (#SETTINGS.componentNames * 40)
    form.Position = "poScreenCenter"

    -- Add label for position name
    local lblName = createLabel(form)
    lblName.Caption = "Position Name:"
    lblName.Left = 20
    lblName.Top = 20
    lblName.Width = 100

    -- Add edit field for position name
    local edtName = createEdit(form)
    edtName.Left = 130
    edtName.Top = 16
    edtName.Width = 160

    -- Create input fields for each coordinate component
    local edits = {}
    for i, component in ipairs(SETTINGS.componentNames) do
        local lblComponent = createLabel(form)
        lblComponent.Caption = component:upper() .. " Coordinate:"
        lblComponent.Left = 20
        lblComponent.Top = 20 + (i * 40)
        lblComponent.Width = 100

        local edtComponent = createEdit(form)
        edtComponent.Left = 130
        edtComponent.Top = 16 + (i * 40)
        edtComponent.Width = 160
        edtComponent.Text = "0.0"
        edits[component] = edtComponent
    end

    -- Add buttons
    local btnOK = createButton(form)
    btnOK.Caption = "Save"
    btnOK.Left = 110
    btnOK.Top = form.Height - 70
    btnOK.Width = 80
    btnOK.Default = true

    -- Add cancel button
    local btnCancel = createButton(form)
    btnCancel.Caption = "Cancel"
    btnCancel.Left = 200
    btnCancel.Top = form.Height - 70
    btnCancel.Width = 80

    local result = nil

    -- Handle OK button click
    btnOK.OnClick = function()
        -- Validate input
        local name = edtName.Text
        if name == "" then
            notification("Position name cannot be empty.", "Error")
            return
        end

        -- Create coordinates table
        local coords = {}
        local invalidInput = false
        for component, edit in pairs(edits) do
            local value = tonumber(edit.Text)
            if value == nil then
                notification("Invalid coordinate value for " .. component:upper(), "Error")
                invalidInput = true
                break
            end
            -- Apply conversion factor - multiply by the coordinate conversion value
            coords[component] = value * CONFIG.coordinateConversion
        end

        if invalidInput then return end

        -- Load existing positions to check if this one exists
        local positions = loadPositions()

        -- Format coordinates for display
        local coordDisplay = ""
        for i, component in ipairs(SETTINGS.componentNames) do
            coordDisplay = coordDisplay .. component .. ": " .. coords[component]
            if i &lt; #SETTINGS.componentNames then
                coordDisplay = coordDisplay .. "\n"
            end
        end

        -- Check if position already exists
        if positions[name] then
            -- Ask outside the form to avoid modal window issues
            form.modalResult = mrOK
            result = {name = name, coords = coords}
        else
            -- Create new position with manual point
            if savePositionToFile(name, coords, false) then
                notification("Position '" .. name .. "' saved successfully!\n" .. coordDisplay, "Success")
                form.modalResult = mrOK
            end
        end
    end

    -- Handle Cancel button click
    btnCancel.OnClick = function()
        form.modalResult = mrCancel
    end

    -- Show modal and wait for result
    form.showModal()

    -- Clean up the form
    form.destroy()

    -- Handle existing position case after form is closed
    if result then
        notification(
            "Position '" .. result.name .. "' already exists. Add as new point or overwrite?",
            "Position Exists",
            {"Add Point", "Overwrite", "Cancel"},
            function(buttonClicked)
                if buttonClicked == "Add Point" then
                    -- Add new point to existing position
                    if savePositionToFile(result.name, result.coords, true) then
                        -- Format coordinates for display
                        local coordDisplay = ""
                        for i, component in ipairs(SETTINGS.componentNames) do
                            coordDisplay = coordDisplay .. component .. ": " .. result.coords[component]
                            if i &lt; #SETTINGS.componentNames then
                                coordDisplay = coordDisplay .. "\n"
                            end
                        end
                        notification("New point added to '" .. result.name .. "' successfully!\n" .. coordDisplay, "Success")
                    end
                elseif buttonClicked == "Overwrite" then
                    -- Overwrite position
                    if savePositionToFile(result.name, result.coords, false) then
                        -- Format coordinates for display
                        local coordDisplay = ""
                        for i, component in ipairs(SETTINGS.componentNames) do
                            coordDisplay = coordDisplay .. component .. ": " .. result.coords[component]
                            if i &lt; #SETTINGS.componentNames then
                                coordDisplay = coordDisplay .. "\n"
                            end
                        end
                        notification("Position '" .. result.name .. "' overwritten successfully!\n" .. coordDisplay, "Success")
                    end
                end
            end
        )
    end
end

