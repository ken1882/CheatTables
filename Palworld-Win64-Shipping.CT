{$LUA}
-- Position Saver Script
-- Saves, loads, and deletes coordinates from memory with configurable dimensions and data types

-- Configuration options
local CONFIG = {
    -- Set coordinate system (can be "2D" for X,Y or "3D" for X,Y,Z)
    dimensionType = "3D",

    -- Set data type (can be "float" or "double")
    dataType = "double",

    -- Base pointer name
    basePtrName = "basePtrPosXYZPtr",

    -- conversion of manually Input variables(read from map) to internal game-version
    coordinateConversion = 100
}

-- Derived settings based on configuration
local SETTINGS = {
    -- Size of each coordinate value in bytes (4 for float, 8 for double)
    valueSize = CONFIG.dataType == "double" and 8 or 4,

    -- Number of coordinates (2 for X,Y or 3 for X,Y,Z)
    dimensions = CONFIG.dimensionType == "2D" and 2 or 3,

    -- Function to read coordinate values (readFloat or readDouble)
    readValue = CONFIG.dataType == "double" and readDouble or readFloat,

    -- Function to write coordinate values (writeFloat or writeDouble)
    writeValue = CONFIG.dataType == "double" and writeDouble or writeFloat,

    -- Coordinate component names
    componentNames = CONFIG.dimensionType == "2D" and {"x", "y"} or {"x", "y", "z"}
}

-- Display configuration info to the user
function showConfigInfo()
    local message = "Position Saver Configuration:\n"
                   .. "- Dimension Type: " .. CONFIG.dimensionType .. "\n"
                   .. "- Data Type: " .. CONFIG.dataType .. "\n"
                   .. "- Value Size: " .. SETTINGS.valueSize .. " bytes\n"
                   .. "- Components: " .. table.concat(SETTINGS.componentNames, ", ") .. "\n"
                   .. "- Coordinate Conversion: " .. CONFIG.coordinateConversion
    notification(message, "Configuration Info")
end

-- File to store positions
local positionFile = "positions.dat"

-- Helper function to read table file content
function readTableFileContent(file)
    local stream = createStringStream()
    stream.copyFrom(file.Stream, file.Stream.Size)
    local lines = {}
    for line in string.gmatch(stream.DataString, "[^\r\n]+") do
        table.insert(lines, line)
    end
    stream.destroy()
    return lines
end

-- Function to get current coordinates
function getCurrentCoordinates()
    local basePtr = readQword(CONFIG.basePtrName)
    if basePtr == 0 then
        return nil
    end

    local coords = {}
    for i, component in ipairs(SETTINGS.componentNames) do
        local offset = (i - 1) * SETTINGS.valueSize
        coords[component] = SETTINGS.readValue(basePtr + offset)
    end

    return coords
end

-- Helper function to write table file content
function writeTableFileContent(file, content)
    file.Stream.Position = 0
    file.Stream.write({string.byte(content, 1, #content)})
end

-- Load positions from the table file
function loadPositions()
    local tf = findTableFile(positionFile)
    if not tf then
        tf = createTableFile(positionFile)
        if tf then
            writeTableFileContent(tf, "")
        else
            notification("Failed to create position data file", "Error")
        end
        return {}
    end

    local posData = {}
    local lines = readTableFileContent(tf)

    for _, line in ipairs(lines) do
        local name, coords = string.match(line, "([^:]+):(.+)")
        if name and coords then
            posData[name] = {}
            for coordPair in string.gmatch(coords, "([^;]+)") do
                local values = {}
                local matches = {}

                -- Parse coordinate values based on dimension count
                for value in string.gmatch(coordPair, "([^,]+)") do
                    table.insert(matches, value)
                end

                if #matches == #SETTINGS.componentNames then
                    local point = {}
                    for i, component in ipairs(SETTINGS.componentNames) do
                        point[component] = tonumber(matches[i])
                    end
                    table.insert(posData[name], point)
                end
            end
        end
    end

    return posData
end

-- Save positions to file, appending new positions
function savePositionToFile(name, coords, isNewPosition)
    local tf = findTableFile(positionFile)
    if not tf then
        tf = createTableFile(positionFile)
        if not tf then
            notification("Failed to create position file.", "Error")
            return false
        end
        writeTableFileContent(tf, "")
    end

    -- Read existing content
    local content = ""
    local lines = {}

    -- Add error handling around readTableFileContent
    local success, result = pcall(function()
        return readTableFileContent(tf)
    end)

    if success then
        lines = result
    else
        notification("Error reading file content: " .. tostring(result), "Debug")
        lines = {}
    end

    -- Format coordinate string
    local coordStr = ""
    for i, component in ipairs(SETTINGS.componentNames) do
        coordStr = coordStr .. coords[component]
        if i &lt; #SETTINGS.componentNames then
            coordStr = coordStr .. ","
        end
    end

    local positionExists = false

    -- Check if position exists and handle accordingly
    for i, line in ipairs(lines) do
        local existingName = string.match(line, "([^:]+):")
        if existingName == name then
            positionExists = true
            if isNewPosition then
                -- Add new point to existing position
                lines[i] = line .. ";" .. coordStr
            else
                -- Overwrite position
                lines[i] = name .. ":" .. coordStr
            end
        end
    end

    -- If position doesn't exist, add it
    if not positionExists then
        table.insert(lines, name .. ":" .. coordStr)
    end

    -- Combine lines into content
    content = table.concat(lines, "\n")

    -- Write content to file with error handling
    local writeSuccess, writeError = pcall(function()
        writeTableFileContent(tf, content)
    end)

    if not writeSuccess then
        notification("Error writing to file: " .. tostring(writeError), "Debug")
        return false
    end

    return true
end

-- Remove a position or point from the file
function removePositionFromFile(name, pointIndex)
    local tf = findTableFile(positionFile)
    if not tf then
        notification("Position file not found.", "Error")
        return false
    end

    -- Read existing content
    local lines = readTableFileContent(tf)
    local newLines = {}
    local deletedSomething = false

    for _, line in ipairs(lines) do
        local existingName, coords = string.match(line, "([^:]+):(.+)")
        if existingName == name then
            if pointIndex then
                -- Delete specific point
                local points = {}
                local count = 0

                -- Split the coordinate string into individual points
                for coordPair in string.gmatch(coords, "([^;]+)") do
                    count = count + 1
                    if count ~= pointIndex then  -- Keep points that aren't the one to delete
                        table.insert(points, coordPair)
                    else
                        deletedSomething = true
                    end
                end

                if #points &gt; 0 then
                    -- Add modified line back if there are remaining points
                    table.insert(newLines, name .. ":" .. table.concat(points, ";"))
                else
                    -- If no points remain, don't add the position back (delete it)
                    deletedSomething = true
                end
            else
                -- Delete entire position
                deletedSomething = true
                -- Don't add this line back (effectively deleting the position)
            end
        else
            -- Keep other positions
            table.insert(newLines, line)
        end
    end

    if not deletedSomething then
        notification("Nothing was deleted. Position or point not found.", "Warning")
        return false
    end

    -- Write content to file (make sure we clear the file first)
    local content = table.concat(newLines, "\n")
    tf.Stream.Size = 0  -- Clear the file first
    writeTableFileContent(tf, content)
    return true
end

