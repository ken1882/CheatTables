            <CheatEntry>
              <ID>106</ID>
              <Description>"Instances Of Selected Entity(All)"</Description>
              <Options moHideChildren="1"/>
              <LastState/>
              <Color>0AF5A3</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>{$lua}
[ENABLE]
if syntaxcheck then return end
if t then object_destroy(t); t=nil end
local MainPTR1 = getAddress('Stronghold2.exe+DB6C58')
local MainPTR2 = getAddress('Stronghold2.exe+6E66D0')
firstBase = readInteger(MainPTR1 + 0x1268)

function calculatingDynamicOffset()
  if not MainPTR1 then return 0; end
  totalNumberOfEntities1 = readInteger(MainPTR1 + 0x126C)
  if not totalNumberOfEntities1 then return end
  entityDifference = totalNumberOfEntities1 - firstBase
  entityCount = entityDifference &gt;&gt; 3
  dynamicOffsets = {}
  for i=0 , entityCount do
      newVal = getAddress(firstBase+ i*8)
      finalBaseToDynamicOffsets = readInteger(newVal+4)
      --print(finalBaseToDynamicOffsets &gt;&gt; 0x10)
      if not finalBaseToDynamicOffsets then return end
      dynamicOffsets[i+1] = finalBaseToDynamicOffsets &gt;&gt; 0x10
  end
  return dynamicOffsets
end

function calculatingWhichEntityIsSelected()
  if not arryOfOffsets then return 0; end
  if #arryOfOffsets == 0 then return 0; end
  secondBase = readInteger(MainPTR2 + 8)
  for i=0, #arryOfOffsets-1 do
      BaseAddressToAllEntities = readPointer(secondBase + arryOfOffsets[i+1]*4)
      if readBytes(BaseAddressToAllEntities + 0x2A8) == 1 then
         return BaseAddressToAllEntities
      elseif readBytes(BaseAddressToAllEntities + 0x2AA) == 1 then
         return BaseAddressToAllEntities
      end
  end
  return 0
end
t = createTimer()
t.Interval = 100
t.OnTimer = function()
  arryOfOffsets = calculatingDynamicOffset()
  if arryOfOffsets == 0 then return end
  baseAddressToSelectedEntity = calculatingWhichEntityIsSelected()
 -- print(getAddress(baseAddressToSelectedEntity), baseAddressToSelectedEntity)
  if baseAddressToSelectedEntity ~= 0 then
    unregisterSymbol('BaseAddress')
    registerSymbol('BaseAddress',baseAddressToSelectedEntity)
  end
  --printf('%X',baseAddressToSelectedEntity)
end
[DISABLE]
object_destroy(t)
t = nil


