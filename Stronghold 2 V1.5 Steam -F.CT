[ENABLE]
if not Auth then Auth=0 end
if Auth == 0 then
 showMessage("Author:  PeaceBeUponYou")
 Auth = 1
end
function attchProc(procName)
  local cticks = 0
  local maxticks = 500
  local clockTime = 100
  if not getProcessIDFromProcessName(procName) then
     messageDialog("Required Process is unavailable!",mtError,mbOK)
     error()
  else while process ~= procName and cticks &lt;= maxticks do
     openProcess(procName)
     sleep(clockTime)
     end
  end
end
--attchProc()
local procName = "Stronghold2.exe"
if process ~= procName then
   rect = messageDialog('Attach Process?' ,mtConfirmation, mbYes, mbNo)
   if rect == mrYes then
      attchProc(procName)
   elseif rect == mrNo then
      error("End is Here!")
      end
end


